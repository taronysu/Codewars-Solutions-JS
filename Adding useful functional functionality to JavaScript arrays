Array.range = function(start, count) {
  let  range = [], i = start;
  
  while( i < start+count ){
    range.push(i);
    i++;
  }
  
  return range;
}

Array.prototype.sum = function() {
  var sum = 0; arr = this.slice();
  while( arr.length > 0 ){
    sum += arr.pop();
  }
  return sum;
}

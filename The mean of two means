https://www.codewars.com/kata/583df40bf30065fa9900010c/train/javascript

function getMean(arr, x, y) {
  if (x > 1 && y > 1 && x <= arr.length && y <= arr.length) {
   let mean1 = 0, mean2 = 0, res;
   let c = arr.slice().reverse();
   for(let i = 0; i < x; i++){
     mean1 += arr[i];
   }
   for(let j=0; j < y; j++){
     mean2 += c[j];
   }
   res = ((mean1/x) + (mean2/y))/2;
   return res;
 }
 return -1;
}

const makePlural = (options, number) => {
if ((number % 100 < 20) && (number % 100 >= 5)) {
    return options[2]
  }
  if (number % 10 === 1) {
    return options[0];
  } else if ((number % 10 > 1) && (number % 10 < 5)) {
    return options[1];
  } else 
    return options[2]
  }

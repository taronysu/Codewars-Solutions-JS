https://www.codewars.com/kata/51dda84f91f5b5608b0004cc/train/javascript

function solution(number){
 let a = [],
     b = [],
     c = [];
  for(let i = 1; i < number; i++){
    if(i % 3 == 0 && i % 5 !== 0) a.push(i);
    if(i%3 != 0 && i%5 == 0) b.push(i);
    if( i % 15 == 0) c.push(i);
    }
  return [a.length, b.length, c.length];
}

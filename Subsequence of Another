https://www.codewars.com/kata/5ab2b6abe78eee7de600361e/train/javascript

function subseq(needle, hay) {
  {     let m = needle.length;
        let n = hay.length;
        let j = 0;
     
        for (let i = 0; i < n && j < m; i++)
            if (needle[j] == hay[i])
                j++;
  
        return (j == m);
    }
}

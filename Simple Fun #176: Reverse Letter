https://www.codewars.com/kata/58b8c94b7df3f116eb00005b/train/javascript

function reverseLetter(str) {
  let new_string = ''
  let alphabet = 'abcdefghijklmnopqrstuvwxyz'
    for(let i of str) {
        if (alphabet.includes(i))
            new_string += i
}
    return new_string.split('').reverse().join('')
  
}

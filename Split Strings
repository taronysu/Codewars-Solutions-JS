https://www.codewars.com/kata/515de9ae9dcfc28eb6000001/train/javascript

function solution(s){
    let listOfPairs = [];
  let pair = '';
  for (let char of s) {
    pair += char;
    if (pair.length === 2) {
      listOfPairs.push(pair);
      pair = '';
    }
  }
  if (pair) {
    listOfPairs.push(pair + '_');
  }
  return listOfPairs;
}

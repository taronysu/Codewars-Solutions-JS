function blackAndWhite(arr){
 let type = ''
  if (!Array.isArray(arr)) type = 'fake'
  else if (arr.includes(5) && arr.includes(13)) type = 'black'
  else if (!(arr.includes(5) && arr.includes(13))) type = 'white'
  return `It\'s a ${type} array`
} 

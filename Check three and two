https://www.codewars.com/kata/5a9e86705ee396d6be000091/train/javascript

function checkThreeAndTwo(array) {
  let a = 0, b = 0, c = 0;
  for(let i=0;i<array.length;i++) {
    if(array[i]=='a') a++;
        if(array[i]=='b') b++;
    if(array[i]=='c') c++;
  }
  if(a==3&&b==2)return true;
  if(a==3&&c==2)return true;
  if(b==3&&a==2)return true;
  if(b==3&&c==2)return true;
  if(c==3&&a==2)return true;
  if(c==3&&b==2)return true;
  return false;
}

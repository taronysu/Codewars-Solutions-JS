const  startingMark = bodyHeight => {
  let m = (10.67 - 9.45) / (1.83 - 1.52)
  return +(((m * bodyHeight + 10.67 - m * 1.83) * 100)/100).toFixed(2)
  // Remember: Body height of 1.52 m --> starting mark: 9.45 m
  //           Body height of 1.83 m --> starting mark: 10.67 m
  }

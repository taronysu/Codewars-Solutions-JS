function aliasGen(f_name,l_name){
   if(f_name[0].toUpperCase() in firstName && l_name[0].toUpperCase() in surname) { 
        x = firstName[f_name[0].toUpperCase()]
        y = surname[l_name[0].toUpperCase()]
        return x +" "+ y
     }
    
    else 
        return 'Your name must start with a letter from A - Z.'
  }

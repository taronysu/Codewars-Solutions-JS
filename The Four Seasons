function fourSeasons(d) {
  if (d>365) {return 'The year flew by!'}
  if (d>=80&&d<=171) {
  return 'Spring Season'
  } else if (d>=172&&d<=263) {
  return 'Summer Season'
  } else if (d>=264&&d<=354) {
   return 'Autumn Season'
}
  return 'Winter Season'
}

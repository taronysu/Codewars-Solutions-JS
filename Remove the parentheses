https://www.codewars.com/kata/5f7c38eb54307c002a2b8cc8/train/javascript

function removeParentheses(s){
  let c = 0
  let  res = ""
    for (let i of s) {
        if (i=="(")
            c += 1 
        else if (i==")")
            c -= 1 
        else {
            if (c == 0)
                res += i 
          }
      }
    return res
}

https://www.codewars.com/kata/51b62bf6a9c58071c600001b/train/javascript

function solution(number){
  let res = '';
   let ROM = {M: 1000, CM: 900, D: 500, CD: 400, C: 100, XC: 90, L: 50, XL: 40, X: 10, IX: 9, V: 5, IV: 4, I: 1};
  for(let el in ROM) {
    while(number >= ROM[el]) {
      res += el;
      number -= ROM[el]
    }
  }
  return res;
}

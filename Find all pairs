https://www.codewars.com/kata/5c55ad8c9d76d41a62b4ede3/train/javascript

function duplicates(array){
  array = array.sort();
  let c = 0
  for (let i = 0; i < array.length; i++){
      if (array[i] === array[i+1]){
        c++;
        array[i] ='';
        array[i+1] ='';
      }
    }
  return c
}

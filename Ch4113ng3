https://www.codewars.com/kata/59e9f404fc3c49ab24000112/train/javascript

function nerdify(txt){
  let res = ''
for(let el of txt) {
  if(el === 'a' || el === 'A') {
    res +='4'
  }
  else if(el === 'e' || el === 'E') {
    res +='3'
  }
  else if (el === 'l') {
    res += '1'
  }
  else {
    res += el
  }
}
  return res
}

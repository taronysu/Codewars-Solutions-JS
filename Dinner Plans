https://www.codewars.com/kata/57212c55b6fa235edc0002a2/train/javascript

function commonGround(s1, s2){
  let res='';
  s1=s1.split(' ');
  s2=s2.split(' ');
  for (let i = 0; i < s1.length; i++){
      if (s1.includes(s2[i])) {
        res += s2[i] + ' ';
        }
    }
  res = res.trim();
  return res.length ? res: 'death';
  }
